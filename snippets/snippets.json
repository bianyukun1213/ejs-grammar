{
  "hexo-theme-tide EJS No Output": {
    "prefix": "tidejs",
    "body": [
      "/% $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS No Output"
  },
  "hexo-theme-tide EJS Output Value": {
    "prefix": "tidejsout",
    "body": [
      "/%= $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS outputs no value"
  },
  "hexo-theme-tide EJS Output Escaped": {
    "prefix": "tidejsesc",
    "body": [
      "/%- $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS outputs value"
  },
  "hexo-theme-tide EJS Comment": {
    "prefix": "tidejscom",
    "body": [
      "/%# $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS comment tag with no output"
  },
  "hexo-theme-tide EJS Literal": {
    "prefix": "tidejslit",
    "body": [
      "/%% $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS outputs a literal '/%'"
  },
  "hexo-theme-tide EJS Include": {
    "prefix": "tidejsinc",
    "body": [
      "/%- include('$1') %/$2"
    ],
    "description": "hexo-theme-tide EJS include statement"
  },
  "hexo-theme-tide EJS For Loop": {
    "prefix": "tidejsfor",
    "body": [
      "/% for( let ${1:index} = 0; ${1:index} < ${2:array}.length; ${1:index}++ ) { %/",
      "$3",
      "/% } %/"
    ],
    "description": "hexo-theme-tide EJS For Loop"
  },
  "hexo-theme-tide EJS ForEach": {
    "prefix": "tidejseach",
    "body": [
      "/% ${1:array}.forEach(${2:element} => { %/",
      " $3",
      "/% }) %/"
    ],
    "description": "hexo-theme-tide EJS ForEach Loop"
  },
  "hexo-theme-tide EJS If Statement": {
    "prefix": "tidejsif",
    "body": [
      "/% if (${1:condition}) { %/",
      " $2",
      "/% } %/"
    ],
    "description": "hexo-theme-tide EJS if statement"
  },
  "hexo-theme-tide EJS Else Statement": {
    "prefix": "tidejselse",
    "body": [
      "/% } else { %/",
      " $1"
    ],
    "description": "hexo-theme-tide EJS if statement"
  },
  "hexo-theme-tide EJS Else If Statement": {
    "prefix": "tidejselif",
    "body": [
      "/% } else if ({$1:condition}) { %/",
      " $2"
    ],
    "description": "hexo-theme-tide EJS if statement"
  },
  "hexo-theme-tide EJS TAG": {
    "prefix": "/%",
    "body": [
      "/% $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS No Output"
  },
  "hexo-theme-tide EJS TAG Output Value": {
    "prefix": "/%=",
    "body": [
      "/%= $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS outputs no value"
  },
  "hexo-theme-tide EJS TAG Output Escaped": {
    "prefix": "/%-",
    "body": [
      "/%- $1 %/$2"
    ],
    "description": "hexo-theme-tide EJS outputs value"
  },
  "hexo-theme-tide EJS TAG Comment": {
